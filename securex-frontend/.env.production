# API Configuration
REACT_APP_API_BASE_URL=https://api.securex.com/v1
REACT_APP_WS_BASE_URL=wss://api.securex.com/realtime

# Authentication
REACT_APP_COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
REACT_APP_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
REACT_APP_COGNITO_DOMAIN=auth.securex.com

# Threat Intelligence
REACT_APP_ALIENVAULT_API_KEY=your_alienvault_key
REACT_APP_VIRUSTOTAL_API_KEY=your_virustotal_key
REACT_APP_ABUSEIPDB_API_KEY=your_abuseipdb_key

# Monitoring
REACT_APP_SENTRY_DSN=https://your_sentry_dsn.ingest.sentry.io/project_id
REACT_APP_GA_TRACKING_ID=GA-XXXXXXXXXX
REACT_APP_HOTJAR_ID=xxxxxxxx

# Feature Flags
REACT_APP_AI_ASSISTANT_ENABLED=true
REACT_APP_ZERO_TRUST_ENABLED=true
REACT_APP_THREAT_PREDICTION_ENABLED=true
REACT_APP_COMPLIANCE_DASHBOARD_ENABLED=true

# Security
REACT_APP_ENCRYPTION_KEY=your_production_encryption_key
REACT_APP_CSP_NONCE=your_csp_nonce

# Performance
REACT_APP_LAZY_LOAD_ENABLED=true
REACT_APP_PREFETCH_ENABLED=true
REACT_APP_CACHE_TTL=300000

# Build Optimization
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=true
INLINE_RUNTIME_CHUNK=false